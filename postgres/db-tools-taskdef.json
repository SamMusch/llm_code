{
  "family": "db-tools",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::571155619056:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::571155619056:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "db-tools",
      "image": "571155619056.dkr.ecr.us-east-1.amazonaws.com/db-tools:latest",
      "essential": true,
      "entryPoint": ["sh","-lc"],
      "command": ["sleep 3600"],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/db-tools",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}